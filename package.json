{
  "private": true,
  "workspaces": [
    "dextra-components",
    "dextra-utils"
  ],
  "scripts": {
    "dev:worker": "pnpm --filter dextra-utils build-worker && pnpm --filter dextra-utils watch",
    "serve:worker": "npx http-server ./dextra-utils/dist -p 8765 -g true -s --cors -c-1",
    "dev:components": "pnpm --filter dextra-components storybook",
    "dev": "concurrently --kill-others \"pnpm dev:worker\"  \"pnpm dev:components\" \"pnpm serve:worker \"",
    "clean": "rm -rf node_modules && rm -rf dextra-components/node_modules && rm -rf dextra-utils/node_modules",
    "plop": "plop"
  },
  "packageManager": "yarn@3.3.1",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@changesets/cli": "^2.26.0",
    "babel-loader": "^9.1.2",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "zx": "^7.1.1",
    "plop": "^3.1.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.13",
    "@loaders.gl/core": "^3.2.13",
    "@loaders.gl/csv": "^3.2.13",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.4",
    "http-server": "^14.1.1",
    "loaders.gl": "^0.3.5"
  }
}
