{
  "private": false,
  "workspaces": [
    "dextra-components",
    "dextra-utils"
  ],
  "scripts": {
    "dev:worker": "pnpm --filter dextra-utils build-worker && pnpm --filter dextra-utils watch",
    "serve:worker": "npx http-server ./dextra-utils/dist -p 8765 -g true -s --cors -c-1",
    "dev:components": "pnpm --filter dextra-components storybook",
    "dev": "concurrently --kill-others \"pnpm dev:worker\"  \"pnpm dev:components\" \"pnpm serve:worker \"",
    "clean": "zx ./scripts/clean.mjs",
    "plop": "plop",
    "set-version": "zx ./scripts/set-version.mjs"
  },
  "packageManager": "yarn@3.3.1",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@changesets/cli": "^2.26.0",
    "babel-loader": "^9.1.2",
    "tsup": "^6.5.0",
    "typescript": "^5.0",
    "zx": "^7.1.1",
    "plop": "^3.1.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.13",
    "@loaders.gl/core": "3.1.8",
    "@loaders.gl/csv": "3.1.8",
    "@loaders.gl/json": "3.1.8",
    "concurrently": "^7.6.0",
    "esbuild": "^0.17.4",
    "http-server": "^14.1.1"
  },
  "version": "0.0.11"
}